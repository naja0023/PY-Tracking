******* */\r\n\r\n   const pageTransition = {\r\n\r\n    in: {\r\n      opacity: 1,\r\n      x: 0,\r\n\r\n    },\r\n    out: {\r\n      opacity: 0,\r\n      x: 100,\r\n\r\n    }\r\n  };\r\n\r\n\r\n  /* ********************************** */\r\n\r\n  return (\r\n    <div>\r\n      <nav className=\"navbar navbar-light bg-light justify-content-between\">\r\n        <Link className=\"buttonblack\" to=\"/\">\r\n          <i className=\"fas fa-angle-left\">\r\n            <a className=\"bl\"></a>\r\n          </i>\r\n        </Link>\r\n        <div className=\"div-img-logo2\">\r\n          <img className=\"img-logoo\" src=\"/images/logo.png\" />\r\n        </div>\r\n      </nav>\r\n\r\n      <motion.div initial=\"out\" animate=\"in\" exit=\"out\" variants={pageTransition}>\r\n      <Loader active={active} />\r\n      <div className=\"history\">\r\n        <table className=\"table table-dark\">\r\n          <thead></thead>\r\n          {History.map((val) => {\r\n            return (\r\n              <tbody>\r\n                <tr>\r\n                  {val.transferor === phone ? (\r\n                    <td key={val.id}>\r\n                      <p className=\"withdraw\"></p>\r\n                      <p className=\"withdraw-detail\">\r\n                        {val.transferor}  {val.recipient}{\" \"}\r\n                      </p>\r\n                    </td>\r\n                  ) : (\r\n                    <td key={val.id}>\r\n                      <p className=\"deposit\"></p>\r\n                      <p className=\"deposit-detail\">\r\n                        {val.transferor}  {val.recipient}{\" \"}\r\n                      </p>\r\n                    </td>\r\n                  )}\r\n\r\n                  {val.transferor === phone ? (\r\n                    <td>\r\n                      <p className=\"amountminus\">- {val.amount}</p>\r\n                      <p className=\"time\">\r\n                        {moment(val.time).format(\"DD/MMMM - LT\")}\r\n                 