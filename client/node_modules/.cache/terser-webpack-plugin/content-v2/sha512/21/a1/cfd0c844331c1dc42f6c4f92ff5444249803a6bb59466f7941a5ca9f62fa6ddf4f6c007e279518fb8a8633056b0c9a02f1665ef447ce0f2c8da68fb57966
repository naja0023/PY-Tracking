hop = () => {\r\n    history.push(\"/myshop\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    axios.get(\"/login\").then((response) => {\r\n      if (response.data.loggedIn == true) {\r\n      }\r\n      if (\r\n        response.data.loggedIn == false ||\r\n        response.data.user[0].role != \"shop\"\r\n      ) {\r\n        return window.location.replace(\"/\");\r\n      }\r\n    });\r\n\r\n    axios.get(\"/getshoplistpromotion\").then((response) => {\r\n      if (response) {\r\n        setShoplist(response.data);\r\n      }\r\n    });\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <nav class=\"navbar navbar-light bg-light justify-content-between\">\r\n        <Link to=\"/\">\r\n          <i className=\"fas fa-angle-left\">\r\n            <a className=\"bl\">ย้อนกลับ</a>\r\n          </i>\r\n        </Link>\r\n      </nav>\r\n      <div className=\"whos-speaking-area speakers pad100\">\r\n        <div className=\"container\">\r\n          <div className=\"row\">\r\n            <div className=\"col-lg-12\">\r\n              <div className=\"section-title text-center\">\r\n                <div className=\"title-text mb50\">\r\n                  <h2>โปรโมชั่นร้านค้า</h2>\r\n                  <input type=\"text\" placeholder=\"search\" />{\" \"}\r\n                  <i className=\"fas fa-search\"></i>\r\n                </div>\r\n                <div\r\n                  className=\"row mb50\"\r\n                  onClick={() => {\r\n                    gotomyshop();\r\n                  }}\r\n                >\r\n                  <div className=\"col-xl-3 col-lg-3 col-md-4 col-sm-12\">\r\n                    <div className=\"speakers xs-mb30\">\r\n                      <div className=\"spk-info\">\r\n                        <p>ร้านค้าของฉัน</p>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr />\r\n                {shoplist.map((val) => {\r\n                  return (\r\n      