ppoint) {\r\n      setTransferStatus(\"คะแนนไม่เพียงพอสำหรับการโอน กรุณาเติมคะแนน\");\r\n    } else if (amount < 1) {\r\n      setTransferStatus(\"ยอดซื้อขั้นต่ำ 50-.\");\r\n    } else {\r\n      axios\r\n        .put(\"/calPointTransfer\", {\r\n          id: id,\r\n          phone: phone,\r\n          destphone: destPhone,\r\n          amount: amount,\r\n        })\r\n        .then((response) => {\r\n          setTransferStatus(response.data.message);\r\n          if (response.data.loggedIn == true) {\r\n            setShopPoint(response.data.user[0].shoppoint);\r\n            window.location.reload();\r\n          }\r\n        });\r\n    }\r\n  };\r\n\r\n  const logout = () => {\r\n    axios.get(\"/logout\");\r\n    window.location.replace(\"/\");\r\n  };\r\n\r\n  const gotoTopupPage = () => {\r\n    history.push(\"/top-up-point-shop\");\r\n  };\r\n\r\n  useEffect(() => {\r\n    axios.get(\"/login\").then((response) => {\r\n      if (response.data.loggedIn == true) {\r\n        setId(response.data.user[0].id);\r\n        setShopPoint(response.data.user[0].shoppoint);\r\n        setPhone(response.data.user[0].phone);\r\n      }\r\n      if (\r\n        response.data.loggedIn == false ||\r\n        response.data.user[0].role != \"shop\"\r\n      ) {\r\n        return window.location.replace(\"/\");\r\n      }\r\n    });\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <nav class=\"navbar navbar-light bg-light justify-content-between\">\r\n        <Link to=\"/\">\r\n          <i className=\"fas fa-angle-left\">\r\n            <a className=\"bl\">ย้อนกลับ</a>\r\n          </i>\r\n        </Link>\r\n      </nav>\r\n      <button onClick={gotoTopupPage} className=\"top-up\">\r\n        <i class=\"fas fa-money-bill-alt\"></i>\r\n        เติมคะแนน\r\n      </button>\r\n      <div className=\"calculate\">\r\n        <h1>คำนวณคะแนน</h1>\r\n        <h1>shoppoint : {shoppoint}</h1>\r\n        <label>กรอกเบอร์</label>\r\n        <input\r\n          type=\"text\"\r\n          