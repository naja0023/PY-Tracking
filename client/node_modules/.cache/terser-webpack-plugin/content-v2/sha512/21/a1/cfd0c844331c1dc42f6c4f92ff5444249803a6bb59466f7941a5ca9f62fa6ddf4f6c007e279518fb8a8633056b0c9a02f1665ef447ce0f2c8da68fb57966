alId] = useState(\"\");\r\n  const [name, setName] = useState(\"\");\r\n  const [phone, setPhone] = useState(\"\");\r\n  const [email, setEmail] = useState(\"\");\r\n  const [point, setPoint] = useState();\r\n  const [role, setRole] = useState(\"\");\r\n\r\n  const [neweNational_id, setnewNationalId] = useState(\"\");\r\n  const [newname, setnewName] = useState(\"\");\r\n  const [newphone, setnewPhone] = useState(\"\");\r\n  const [newemail, setnewEmail] = useState(\"\");\r\n\r\n  axios.defaults.withCredentials = true;\r\n  const logout = () => {\r\n    axios.get(\"/logout\");\r\n    window.location.replace(\"/\");\r\n  };\r\n  const refresh = () => {\r\n    axios.get(\"/member\");\r\n    window.location.replace(\"/memberprofile\");\r\n  };\r\n\r\n  const idUpdate = () => {\r\n    axios\r\n      .put(\"/updatememberId\", { national_id: neweNational_id, id: id })\r\n      .then((response) => {\r\n        if (response.data.message) {\r\n          return alert(response.data.message);\r\n        } else {\r\n          alert(\"บันทึกสำเร็จ\");\r\n          return setNationalId(response.data.user[0].national_id);\r\n        }\r\n      });\r\n  };\r\n\r\n  const nameUpdate = () => {\r\n    if (newname == \"\") {\r\n      alert(\"ชื่อไม่ถูกต้อง\");\r\n    } else {\r\n      axios\r\n        .put(\"/updatememberName\", { name: newname, id: id })\r\n        .then((response) => {\r\n          alert(\"บันทึกสำเร็จ\");\r\n          return setName(response.data.user[0].name);\r\n        });\r\n    }\r\n  };\r\n  const phoneUpdate = () => {\r\n    if (newphone == \"\") {\r\n      alert(\"เบอร์โทรไม่ถูกต้อง\");\r\n    } else {\r\n      axios\r\n        .put(\"/updatememberPhone\", { phone: newphone, id: id })\r\n        .then((response) => {\r\n          if (response.data.message) {\r\n            return alert(response.data.message);\r\n          } else {\r\n            alert(\"บันทึกสำเร็จ\");\r\n            return setPhone(response.data.user[0].phone);\r\n          }\r\n        });