onse) => {\r\n        if (response.data.message) {\r\n          return alert(response.data.message);\r\n        } else {\r\n          setEmail(response.data.user[0].email);\r\n        }\r\n      });\r\n  };\r\n\r\n  useEffect(() => {\r\n    axios.get(\"/login\").then((response) => {\r\n      if (response.data.loggedIn == true) {\r\n        setId(response.data.user[0].id);\r\n        setNationalId(response.data.user[0].national_id);\r\n        setName(response.data.user[0].name);\r\n        setPhone(response.data.user[0].phone);\r\n        setEmail(response.data.user[0].email);\r\n        setPoint(response.data.user[0].point);\r\n        setShoptype(response.data.user[0].type);\r\n        setShopPoint(response.data.user[0].shoppoint);\r\n        setRole(response.data.user[0].role);\r\n      }\r\n      if (\r\n        response.data.loggedIn == false ||\r\n        response.data.user[0].role != \"shop\"\r\n      ) {\r\n        return window.location.replace(\"/\");\r\n      }\r\n    });\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"shopprofile-page\">\r\n      <nav class=\"navbar navbar-light bg-light justify-content-between\">\r\n        <Link to=\"/\">\r\n          <i className=\"fas fa-angle-left\">\r\n            <a className=\"bl\">ย้อนกลับ</a>\r\n          </i>\r\n        </Link>\r\n      </nav>\r\n\r\n      <div>\r\n        <Modal\r\n          isOpen={modalIsOpenid}\r\n          onAfterOpen={afterOpenModal}\r\n          onRequestClose={closeModal}\r\n          style={customStyles}\r\n          contentLabel=\"Example Modal\"\r\n        >\r\n          <h2 ref={(_subtitle) => (subtitle = _subtitle)}>\r\n            เลขบัตรประจำตัวประชาชน\r\n          </h2>\r\n          <form>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"id\"\r\n              onChange={(e) => {\r\n                setnewNationalId(e.target.value);\r\n              }}\r\n            />\r\n            <button className=\"btn\" onClick={idUpdate}>\r\n      