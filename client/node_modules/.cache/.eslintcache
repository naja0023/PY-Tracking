_error[j].transferor)\r\n          if (shop[i].phone === checkTx_error[j].transferor) {\r\n            withdraw += checkTx_error[j].amount\r\n          } else if (shop[i].phone === checkTx_error[j].recipient) {\r\n            deposit += checkTx_error[j].amount\r\n          }\r\n        }\r\n        // console.log(member[i].point)\r\n        // console.log(deposit - withdraw)\r\n        //check คะแนนใน block เทียบกับ DB ของ member\r\n        if (deposit - withdraw !== shop[i].point + shop[i].shoppoint) {\r\n          setshop_error([\r\n            {\r\n              name: shop[i].name,\r\n              phone: shop[i].phone,\r\n              pointDB: shop[i].point,\r\n              shoppointDB: shop[i].shoppoint,\r\n              pointBC: deposit - withdraw,\r\n              role: shop[i].role,\r\n            },\r\n            ...shop_error,\r\n          ])\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  const zoomtransactionpiv = () => {}\r\n  /* ********************************** */\r\n\r\n  const pageTransition = {\r\n    in: {\r\n      opacity: 1,\r\n      x: 0,\r\n    },\r\n    out: {\r\n      opacity: 0,\r\n      x: '-100vh',\r\n    },\r\n  }\r\n\r\n  /* ********************************** */\r\n\r\n  useEffect(() => {\r\n    const timer = setTimeout(() => {\r\n      setactive(false)\r\n      axios.get('/login').then((response) => {\r\n        if (\r\n          response.data.loggedIn === true &&\r\n          response.data.user[0].role === 'admin'\r\n        ) {\r\n          axios.get('/admin').then((response) => {\r\n            setname(response.data.user[0].name)\r\n            setpoint(response.data.user[0].point)\r\n          })\r\n          axios.get('/getpromotionrequest').then((response) => {\r\n            if (response.data) {\r\n              setpendingpromotion(response.data)\r\n            }\r\n          })\r\n          axios.get('/gettopuprequest').then((response) => {\r\n            if (response.data) {\r\n              setTopupReq(response.data)\r\n